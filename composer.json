{
    "name": "roave/api-compare",
    "description": "Tool to compare two revisions of a public API to check for BC breaks",
    "require": {
        "php": "^7.2",
        "beberlei/assert": "^2.9",
        "composer/composer": "^1.6",
        "nikolaposa/version": "^2.2",
        "roave/better-reflection": "dev-master",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^4.0",
        "symfony/process": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "doctrine/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpstan/phpstan": "^0.9.2",
        "infection/infection": "^0.8.1"
    },
    "autoload": {
        "psr-4": {
            "Roave\\ApiCompare\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoaveTest\\ApiCompare\\": "test/unit"
        }
    }
}
