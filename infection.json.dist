{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php://stderr",
        "github": true,
        "badge": {
            "branch": "/^\\d+\\.\\d+\\.x$/"
        }
    },
    "mutators": {
        "@default": true,
        "IdenticalEqual": false,
        "NotIdenticalNotEqual": false
    },
    "minMsi": 100,
    "minCoveredMsi": 100
}
